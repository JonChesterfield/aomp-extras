#%Module1.0
##
## aomp@X.Y-Z
##
module-whatis "aomp @X.Y-Z"

proc ModulesHelp { } {
puts stderr "This version of AOMP contains C/C++/Flang."
}

prepend-path PATH "/lib/lib/aomp-X.Y-Z/bin"
prepend-path CMAKE_PREFIX_PATH "/lib/lib/aomp-X.Y-Z"
prepend-path LIBRARY_PATH "/lib/lib/aomp-X.Y-Z/lib"
prepend-path LD_LIBRARY_PATH "/lib/lib/aomp-X.Y-Zx/lib"
prepend-path LD_RUN_PATH "/lib/lib/aomp-X.Y-Z/lib"
# not sure what to put here: prepend-path PKG_CONFIG_PATH "/lib/lib/aomp-X.Y-Z/lib/pkgconfig"
prepend-path MANPATH "/lib/lib/aomp-X.Y-Z/share/man"
prepend-path CPATH "/lib/lib/aomp-X.Y-Z/include"
setenv AOMP_ROOT "/lib/lib/aomp-X.Y-Z"
setenv AOMP "/lib/lib/aomp-X.Y-Z"



